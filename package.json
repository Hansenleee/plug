{
  "name": "plug-proxy",
  "version": "0.0.1-beta.23",
  "description": "An http proxy tool",
  "type": "module",
  "main": "electron/main.js",
  "files": [
    "bin",
    "dist",
    "ui-dist",
    "resources",
    "open"
  ],
  "scripts": {
    "dev": "rm -rf ./dist && cross-env NODE_ENV=dev rollup -c -w",
    "dev:watch": "nodemon",
    "dev:plug": "cross-env NODE_ENV=dev node bin/plug.js start -d",
    "dev:plug:run": "cross-env NODE_ENV=dev node bin/plug.js run -d",
    "build": "rm -rf ./dist && cross-env NODE_ENV=prod rollup -c",
    "build:analyze": "cross-env NODE_ENV=prod BUILD_ANALYZE=true rollup -c",
    "start:ui": "cd ui && yarn start",
    "build:ui": "cd ui && yarn build",
    "start:app": "electron .",
    "start:forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:win32": "yarn make -- --platform win32",
    "make:darwin": "yarn make -- --platform darwin",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "icon": "resources/images/logo.icns",
    "mac": {
      "icon": "resources/images/logo.icns"
    }
  },
  "bin": {
    "plug": "./bin/plug.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hansenleee/plug.git"
  },
  "keywords": [
    "proxy"
  ],
  "author": "lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hansenleee/plug/issues"
  },
  "homepage": "https://github.com/Hansenleee/plug#readme",
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/http-proxy": "^1.17.14",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.12.12",
    "@types/node-forge": "^1.3.11",
    "cross-env": "6.0.3",
    "electron": "^32.0.1",
    "eslint": "^8.3.0",
    "eslint-config-ali": "^15.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.1.7",
    "prettier": "~2.8.7",
    "rollup": "^4.17.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.4.5",
    "vitepress": "^1.4.1"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@ladjs/koa-views": "^9.0.0",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^12.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "inquirer": "^10.1.8",
    "koa": "^2.15.3",
    "koa-static": "^5.0.0",
    "log4js": "^6.9.1",
    "lru-cache": "^10.3.0",
    "mockjs": "^1.1.0",
    "nanoid": "^5.0.7",
    "node-fetch": "^2.6.7",
    "node-forge": "^1.3.1",
    "node-localstorage": "3.0.5",
    "ora": "^8.0.1",
    "parse-domain": "^2.3.2",
    "proxy-agent": "^3.1.1",
    "raw-body": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "socket.io": "^4.7.5",
    "typedi": "^0.10.0",
    "v1": "^1.0.0"
  },
  "resolutions": {
    "eslint-plugin-react": "7.34.3"
  }
}
