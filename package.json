{
  "name": "plug-proxy",
  "version": "0.0.1-beta.0",
  "description": "An http proxy tool",
  "type": "module",
  "main": "index.js",
  "files": [
    "bin",
    "dist",
    "ui/build"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=dev rollup -c -w",
    "dev:plug": "node bin/plug.js start -d",
    "build": "cross-env NODE_ENV=prod rollup -c",
    "build:analyze": "cross-env NODE_ENV=prod BUILD_ANALYZE=true rollup -c",
    "start:ui": "cd ui && yarn start",
    "build:ui": "cd ui && yarn build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "plug": "./bin/plug.js"
  },
